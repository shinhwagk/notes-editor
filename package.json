{
  "name": "vscode-note",
  "displayName": "vscode-note",
  "description": "save note by json file",
  "version": "0.1.1",
  "publisher": "shinhwagk",
  "engines": {
    "vscode": "^1.17.0"
  },
  "categories": [
    "Other"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/shinhwagk/vscode-note"
  },
  "activationEvents": [
    "workspaceContains:.index.json"
  ],
  "main": "./out/extension",
  "contributes": {
    "menus": {
      "view/title": [
        {
          "command": "extension.insert.label",
          "when": "view == vsnote",
          "group": "vsnote"
        }
      ],
      "view/item/context": [
        {
          "command": "extension.insert.category",
          "when": "view == vsnote",
          "group": "vsnote"
        },
        {
          "command": "extension.insert.label",
          "when": "view == vsnote",
          "group": "vsnote"
        }
      ]
    },
    "commands": [
      {
        "command": "extension.showVsNotePreview",
        "title": "Show Vscode Note"
      },
      {
        "command": "extension.insert.category",
        "title": "vsnote: insert category"
      },
      {
        "command": "extension.insert.label",
        "title": "vsnote: insert label"
      }
    ],
    "views": {
      "explorer": [
        {
          "id": "vsnote",
          "name": "vsnote"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^5.0.0",
    "@types/node": "^9.6.5",
    "typescript": "^2.8.1",
    "vscode": "^1.1.14",
    "tslint": "^5.9.1"
  },
  "dependencies": {}
}