{
  "name": "vscode-note",
  "displayName": "vscode-note",
  "description": "note by file",
  "version": "0.0.2",
  "publisher": "shinhwagk",
  "engines": {
    "vscode": "^1.17.0"
  },
  "categories": [
    "Other"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/shinhwagk/vscode-note"
  },
  "activationEvents": [
    "onCommand:extension.showVscodeNotePreview",
    "onCommand:extension.addNote",
    "onCommand:extension.modifyNote",
    "onCommand:extension.deleteNote",
    "onCommand:extension.addCategory",
    "onCommand:extension.modifyCategory",
    "onCommand:extension.deleteCategory",
    "onView:note",
    "onCommand:extension.sayHello"
  ],
  "main": "./out/extension",
  "contributes": {
    "menus": {
      "view/item/context": [
        {
          "command": "extension.addCategory",
          "when": "view == note"
        }
      ]
    },
    "commands": [
      {
        "command": "extension.showVscodeNotePreview",
        "title": "Show Vscode Note"
      },
      {
        "command": "extension.addNote",
        "title": "add vsnote"
      },
      {
        "command": "extension.addCategory",
        "title": "note: add category"
      }
    ],
    "views": {
      "explorer": [
        {
          "id": "note",
          "name": "note"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.44",
    "@types/node": "^8.0.53",
    "awesome-typescript-loader": "^3.4.1",
    "typescript": "^2.6.2",
    "vscode": "^1.1.9"
  },
  "dependencies": {}
}