{
  "name": "vscode-note",
  "displayName": "vscode-note",
  "description": "save note by json file",
  "version": "0.0.4",
  "publisher": "shinhwagk",
  "engines": {
    "vscode": "^1.17.0"
  },
  "categories": [
    "Other"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/shinhwagk/vscode-note"
  },
  "activationEvents": [
    "onCommand:extension.showVscodeNotePreview",
    "onCommand:extension.addNote",
    "onCommand:extension.modifyNote",
    "onCommand:extension.deleteNote",
    "onCommand:extension.addCategory",
    "onCommand:extension.modifyCategory",
    "onCommand:extension.deleteCategory",
    "onView:vsnote",
    "onCommand:extension.sayHello"
  ],
  "main": "./out/extension",
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "VsNote",
      "properties": {
        "vsnote.switch": {
          "type": "boolean",
          "default": true,
          "scope": "resource",
          "description": "......"
        }
      }
    },
    "menus": {
      "view/item/context": [
        {
          "command": "extension.addCategory",
          "when": "view == vsnote"
        },
        {
          "command": "extension.addLabel",
          "when": "view == vsnote"
        }
      ]
    },
    "commands": [
      {
        "command": "extension.showVsNotePreview",
        "title": "Show Vscode Note"
      },
      {
        "command": "extension.addNote",
        "title": "add vsnote"
      },
      {
        "command": "extension.addCategory",
        "title": "vsnote: add Category"
      },
      {
        "command": "extension.addLabel",
        "title": "vsnote: add Label"
      }
    ],
    "views": {
      "explorer": [
        {
          "id": "vsnote",
          "name": "vsnote"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.46",
    "@types/node": "^9.3.0",
    "awesome-typescript-loader": "^3.4.1",
    "typescript": "^2.6.2",
    "vscode": "^1.1.10"
  },
  "dependencies": {}
}